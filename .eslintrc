{
  "root": true,
  "env": {
    "node": true,
    "browser": true
  },
  "extends": [
    "standard",
    "plugin:vue/essential"
  ],
  "rules": {
    "no-console": 0,
    "no-undef": 0,
    "vue/multi-word-component-names": 0
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  }
}
